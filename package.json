{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "book a meal",
  "main": "server.js",
  "scripts": {
    "start": "babel-watch ./server/server.js",
    "devserver": "concurrently \"npm start\" \"npm run webpack:watch\"",
    "test": "npm run migrations && npm run start:test",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "NODE_ENV=production && NODE_END=production && npm run migrations",
    "start:test": "nyc mocha --timeout 50000 --exit --require babel-core/register server/test/test/index.text.js",
    "webpack:watch": "webpack --watch",
    "webpack:start": "webpack-dev-server --progress --colors",
    "webpack:build": "./node_modules/.bin/webpack",
    "build-production": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors"
  },
